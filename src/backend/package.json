{
  "name": "docshield-ai-voice-agent-worker",
  "version": "1.0.0",
  "private": true,
  "description": "Autonomous AI Voice Agent worker service for DocShield's outbound sales calls",
  "main": "dist/index.js",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "check-types": "tsc --noEmit",
    "validate": "npm run lint && npm run test && npm run check-types"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0",
    "@google-cloud/calendar": "^3.x",
    "@googleapis/calendar": "^3.0.0",
    "@logtail/node": "^0.1.0",
    "@logtail/winston": "^0.4.0",
    "bull": "^4.10.0",
    "chalk": "^4.1.2",
    "commander": "^10.0.0",
    "correlation-id": "^5.0.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "16.x",
    "events": "^1.0.0",
    "google-auth-library": "^8.0.0",
    "ioredis": "^5.3.0",
    "leaky-bucket": "^2.0.0",
    "libphonenumber-js": "^1.10.0",
    "livekit-server-sdk": "latest",
    "mongoose": "^7.0.0",
    "node-dtmf": "^0.1.0",
    "node-opus": "^0.3.3",
    "node-wav": "^0.1.0",
    "openai": "latest",
    "opossum": "^6.0.0",
    "retry": "^0.13.0",
    "token-bucket": "^1.0.0",
    "tsyringe": "^4.7.0",
    "uuid": "^9.0.0",
    "validator": "^13.0.0",
    "winston": "^3.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.0.0",
    "@jest/types": "^29.0.0",
    "@types/node": "^18.x",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "husky": "^8.0.0",
    "ioredis-mock": "^8.0.0",
    "jest": "^29.0.0",
    "jest-mock": "^29.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.0"
  }
}